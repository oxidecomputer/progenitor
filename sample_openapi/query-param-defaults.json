{
  "openapi": "3.0.0",
  "info": {
    "title": "Default Parameter Test API",
    "version": "1.0.0"
  },
  "paths": {
    "/upload": {
      "post": {
        "operationId": "upload_file",
        "summary": "Upload a file",
        "parameters": [
          {
            "name": "uploadType",
            "in": "query",
            "required": false,
            "schema": {
              "type": "string",
              "enum": ["multipart"],
              "default": "multipart"
            }
          },
          {
            "name": "supportsAllDrives",
            "in": "query",
            "required": false,
            "schema": {
              "type": "boolean"
            }
          },
          {
            "name": "requiredParam",
            "in": "query",
            "required": true,
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Success",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "status": {
                      "type": "string"
                    }
                  }
                }
              }
            }
          }
        }
      }
    }
  }
}
